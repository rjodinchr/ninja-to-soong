cc_defaults {
    name: "OpenCL-CTS-defaults",
    cflags: [
        "-DCL_ENABLE_BETA_EXTENSIONS",
        "-DCL_NO_EXTENSION_PROTOTYPES",
        "-DCL_TARGET_OPENCL_VERSION=300",
        "-DCL_USE_DEPRECATED_OPENCL_1_0_APIS=1",
        "-DCL_USE_DEPRECATED_OPENCL_1_1_APIS=1",
        "-DCL_USE_DEPRECATED_OPENCL_1_2_APIS=1",
        "-DCL_USE_DEPRECATED_OPENCL_2_0_APIS=1",
        "-DCL_USE_DEPRECATED_OPENCL_2_1_APIS=1",
        "-DCL_USE_DEPRECATED_OPENCL_2_2_APIS=1",
    ],
    local_include_dirs: [
        "test_common",
        "test_common/gl",
        "test_common/gles",
        "test_common/harness",
    ],
    static_libs: ["OpenCL-CTS_test_common_libharness_a"],
    shared_libs: ["//external/OpenCL-ICD-Loader:libOpenCL"],
    defaults: ["OpenCL-CTS-manual-defaults"],
}

cc_defaults {
    name: "OpenCL-CTS-manual-defaults",
    header_libs: [
        "OpenCL-Headers",
        "SPIRV-Headers-includes",
    ],
    cflags: [
        "-Wno-error",
        "-Wno-c++11-narrowing",
        "-Wno-non-virtual-dtor",
        "-Wno-string-concatenation",
        "-fexceptions",
    ],
    gtest: false,
    test_options: {
        unit_test: false,
    },
}

